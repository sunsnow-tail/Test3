<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <appSettings>
    <add key="cityUrl" value="https://restcountries.eu/rest/v2/all" />
    <add key="altService" value="http://samples.openweathermap.org/data/2.5/weather?q={0}" />
