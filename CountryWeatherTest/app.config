    <client>
      <endpoint address="http://www.webservicex.net/globalweather.asmx" binding="basicHttpBinding" bindingConfiguration="GlobalWeatherSoap" contract="GlobalWeatherService.GlobalWeatherSoap" name="GlobalWeatherSoap" />
